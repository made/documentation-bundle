{
  "name": "made/documentation-bundle",
  "description": "Generate static documentation from markdown files. The bundle.",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": "^7.2.5",
    "erusev/parsedown": "^1.7",
    "gameplayjdk/php-static-help": "^1.1",
    "symfony/http-kernel": "^5.0",
    "symfony/console": "^5.0",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/twig-bundle": "^5.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5.0"
  },
  "authors": [
    {
      "name": "GameplayJDK",
      "email": "github@gameplayjdk.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Made\\DocumentationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Made\\DocumentationBundle\\Tests\\": "tests/"
    }
  }
}
